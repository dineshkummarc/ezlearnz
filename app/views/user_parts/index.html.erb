<h1>Listing user_parts</h1>

<table>
  <tr>
    <th>Part</th>
    <th>User unit</th>
    <th>Instructor</th>
    <th>Grade</th>
    <th>Completed if student</th>
    <th>Submission or notes</th>
  </tr>

<% for user_part in @user_parts %>
  <tr>
    <td><%=h user_part.part_id %></td>
    <td><%=h user_part.user_unit_id %></td>
    <td><%=h user_part.instructor %></td>
    <td><%=h user_part.grade %></td>
    <td><%=h user_part.completed_if_student %></td>
    <td><%=h user_part.submission_or_notes %></td>
    <td><%= link_to 'Show', user_part %></td>
    <td><%= link_to 'Edit', edit_user_part_path(user_part) %></td>
    <td><%= link_to 'Destroy', user_part, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user_part', new_user_part_path %>
